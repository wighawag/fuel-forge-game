{
  "name": "fuel-forge-game-onchain",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/lib/index.d.ts",
      "default": "./dist/esm/lib/index.js"
    },
    "./generated": {
      "types": "./dist/esm/src/index.d.ts",
      "default": "./dist/esm/src/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.2.1",
    "as-soon": "^0.0.11",
    "fuel-ts-hasher": "^0.0.1",
    "fuels": "^0.101.3",
    "ldenv": "^0.3.12",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "node delete_proxy_address.js && fuels dev",
    "fuel-core": "node delete_proxy_address.js && fuels node",
    "contracts:dev": "as-soon -w contract/src pnpm build:deploy",
    "test": "as-soon -w contract/src forc test",
    "build:deploy": "fuels build && pnpm typescript && fuels deploy",
    "build": "fuels build && pnpm typescript",
    "typescript": "node fix-imports.js && tsc --project typescript/tsconfig.json"
  }
}
